{
  "name": "gtm-vision",
  "version": "1.0.0",
  "description": "a Web App to analyze GTM containers",
  "main": "server/server.js",
  "scripts": {
    "serve-public": "parcel watch src/index.html --out-dir public --log-level 4",
    "serve-node": "nodemon --watch server --exec babel-node --inspect server/server.js",
    "build-node": "babel server -d dist --no-comments -s",
    "build-public": "parcel build src/index.html --out-dir public --no-source-maps",
    "build": "npm run build-node && npm run build-public",
    "start": "node dist/server.js",
    "gcp-build": "npm run build-node && npm run build-public",
    "firestore-emulator": "gcloud beta emulators firestore start --host-port=localhost:8010",
    "deploy": "eslint ./ && gcloud app deploy -q",
    "lint": "eslint ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JJMeijer/gtm-vision.git"
  },
  "keywords": [
    "Google",
    "Tag",
    "Manager",
    "GTM",
    "Analysis"
  ],
  "author": "Johan Meijer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jjmeijer/gtm-vision/issues"
  },
  "homepage": "https://github.com/jjmeijer/gtm-vision#readme",
  "browserList": [
    "last 3 versions",
    "ie9"
  ],
  "dependencies": {
    "@google-cloud/debug-agent": "^4.2.2",
    "@google-cloud/firestore": "^4.1.1",
    "@google-cloud/logging-winston": "^3.0.6",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@types/react": "^16.9.43",
    "bent": "^7.3.7",
    "compression": "^1.7.4",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "js-beautify": "^1.11.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "prismjs": "^1.20.0",
    "prop-types": "^15.7.2",
    "puppeteer": "^2.1.1",
    "puppeteer-extra": "^3.1.12",
    "puppeteer-extra-plugin-stealth": "^2.4.13",
    "querystring": "^0.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-window": "^1.8.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@material-ui/codemod": "^4.5.0",
    "acorn": "^7.3.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-prismjs": "^2.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "nodemon": "^1.19.4",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "react-hot-loader": "^4.12.21"
  }
}
