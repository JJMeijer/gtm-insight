{
  "name": "gtm-insight",
  "version": "1.0.0",
  "description": "a Web App to analyze GTM containers",
  "main": "server/server.js",
  "scripts": {
    "serve-public": "parcel watch src/index.html --out-dir public --log-level 4",
    "serve-node": "nodemon --watch server --exec babel-node server/server.js --inspect",
    "build-node": "babel server -d dist --no-comments -s",
    "build-public": "parcel build src/index.html --out-dir public --no-source-maps",
    "build": "npm run build-node && npm run build-public",
    "start": "node dist/server.js",
    "gcp-build": "npm run build-node && npm run build-public",
    "firestore-emulator": "gcloud beta emulators firestore start --host-port=localhost:8010",
    "deploy": "eslint ./ && gcloud app deploy -q"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JJMeijer/gtm-insight.git"
  },
  "keywords": [
    "Google",
    "Tag",
    "Manager",
    "GTM"
  ],
  "author": "Johan Meijer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jjmeijer/gtm-insight/issues"
  },
  "homepage": "https://github.com/jjmeijer/gtm-insight#readme",
  "browserList": "> 0.25%, not dead",
  "dependencies": {
    "@google-cloud/debug-agent": "^4.2.2",
    "@google-cloud/firestore": "^3.7.4",
    "@google-cloud/logging-winston": "^3.0.6",
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "@types/react": "^16.9.34",
    "bent": "^7.3.0",
    "compression": "^1.7.4",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "js-beautify": "^1.11.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.0",
    "prismjs": "^1.20.0",
    "prop-types": "^15.7.2",
    "puppeteer": "^2.1.1",
    "puppeteer-extra": "^3.1.9",
    "puppeteer-extra-plugin-stealth": "^2.4.9",
    "querystring": "^0.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-window": "^1.8.5",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "acorn": "^7.1.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-prismjs": "^2.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "nodemon": "^1.19.4",
    "parcel-bundler": "^1.12.4",
    "react-hot-loader": "^4.12.20"
  }
}
