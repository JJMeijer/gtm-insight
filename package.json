{
  "name": "gtm-vision",
  "version": "1.0.0",
  "description": "a Web App to analyze GTM containers",
  "main": "server/server.js",
  "scripts": {
    "serve-public": "parcel watch src/index.html --out-dir public --log-level 4",
    "serve-node": "ts-node-dev server/server.ts --watch server",
    "build-node": "tsc",
    "build-public": "parcel build src/index.html --out-dir public --no-source-maps",
    "build": "npm run build-node && npm run build-public",
    "start": "node dist/server.js",
    "gcp-build": "npm run build-node && npm run build-public",
    "firestore-emulator": "gcloud beta emulators firestore start --host-port=localhost:8010",
    "deploy": "eslint ./ && gcloud app deploy -q",
    "lint": "eslint ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JJMeijer/gtm-vision.git"
  },
  "keywords": [
    "Google",
    "Tag",
    "Manager",
    "GTM",
    "Analysis"
  ],
  "author": "Johan Meijer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jjmeijer/gtm-vision/issues"
  },
  "homepage": "https://github.com/jjmeijer/gtm-vision#readme",
  "browserList": [
    "last 3 versions",
    "ie9"
  ],
  "dependencies": {
    "@google-cloud/debug-agent": "^5.1.2",
    "@google-cloud/firestore": "^4.1.2",
    "@google-cloud/logging-winston": "^4.0.0",
    "@types/bent": "^7.3.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.7",
    "@types/helmet": "0.0.47",
    "@types/js-beautify": "^1.11.0",
    "@types/morgan": "^1.9.1",
    "bent": "^7.3.9",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "js-beautify": "^1.11.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "puppeteer-extra": "^3.1.13",
    "puppeteer-extra-plugin-stealth": "^2.4.14",
    "ts-custom-error": "^3.1.1",
    "typescript": "^3.9.7",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.56"
  }
}
