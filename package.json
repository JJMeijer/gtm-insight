{
  "name": "gtm-vision",
  "version": "1.0.0",
  "description": "a Web App to analyze GTM containers",
  "browser": "public/index.html",
  "scripts": {
    "dev-server": "ts-node-dev server/server.ts --watch server",
    "build-server": "tsc -p ./tsconfig.server.json",
    "dev-frontend": "parcel watch src/index.html --target=browser --dist-dir ./public",
    "build-frontend": "parcel build src/index.html --target=browser --dist-dir ./public --no-source-maps",
    "build": "npm run build-server && npm run build-frontend",
    "gcp-build": "npm run build",
    "start": "node ./dist/server.js",
    "firestore-emulator": "gcloud beta emulators firestore start --host-port=localhost:8010",
    "test": "tsc --noEmit && eslint ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JJMeijer/gtm-vision.git"
  },
  "keywords": [
    "Google",
    "Tag",
    "Manager",
    "GTM",
    "Analysis"
  ],
  "author": "Johan Meijer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jjmeijer/gtm-vision/issues"
  },
  "homepage": "https://github.com/jjmeijer/gtm-vision#readme",
  "browserList": [
    "last 3 versions",
    "ie9"
  ],
  "dependencies": {
    "@google-cloud/debug-agent": "^5.1.2",
    "@google-cloud/firestore": "^4.2.0",
    "@google-cloud/logging-winston": "^4.0.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@types/prismjs": "^1.16.1",
    "bent": "^7.3.10",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "js-beautify": "^1.13.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "prismjs": "^1.21.0",
    "puppeteer": "^5.2.1",
    "puppeteer-extra": "^3.1.15",
    "puppeteer-extra-plugin-stealth": "^2.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-window": "^1.8.5",
    "redux": "^4.0.5",
    "ts-custom-error": "^3.1.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/bent": "^7.3.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.7",
    "@types/helmet": "0.0.47",
    "@types/js-beautify": "^1.11.0",
    "@types/morgan": "^1.9.1",
    "@types/parcel-env": "0.0.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-window": "^1.8.2",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-prismjs": "^2.0.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "parcel": "^2.0.0-nightly.376",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.59",
    "typescript": "^3.9.7"
  }
}
