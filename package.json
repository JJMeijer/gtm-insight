{
  "name": "gtm-vision",
  "version": "1.0.0",
  "description": "a Web App to analyze GTM containers",
  "browser": "public/index.html",
  "scripts": {
    "dev-server": "ts-node-dev server/server.ts --watch server",
    "build-server": "tsc -p ./tsconfig.server.json",
    "dev-frontend": "parcel watch src/index.html --target=browser --dist-dir ./public",
    "build-frontend": "parcel build src/index.html --target=browser --dist-dir ./public --no-source-maps",
    "build": "npm run build-server && npm run build-frontend",
    "start": "node ./dist/server.js",
    "test": "tsc --noEmit && eslint ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JJMeijer/gtm-vision.git"
  },
  "keywords": [
    "Google",
    "Tag",
    "Manager",
    "GTM",
    "Analysis"
  ],
  "author": "Johan Meijer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jjmeijer/gtm-vision/issues"
  },
  "homepage": "https://github.com/jjmeijer/gtm-vision#readme",
  "browserList": [
    "last 3 versions",
    "ie9"
  ],
  "dependencies": {
    "bent": "^7.3.12",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "puppeteer-extra": "^3.1.18",
    "ts-custom-error": "^3.2.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bent": "^7.3.2",
    "@types/compression": "^1.7.1",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "esbuild": "^0.12.19",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  }
}
