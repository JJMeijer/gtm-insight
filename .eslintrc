{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-restricted-imports": ["error", {
        "patterns": ["@material-ui/*/*/*", "!@material-ui/core/test-utils/*"]
    }]
  }
}
