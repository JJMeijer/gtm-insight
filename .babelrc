{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": [
    "react-hot-loader/babel",
    ["prismjs", {
      "languages": ["javascript", "html"],
      "plugins": ["line-numbers", "show-language"],
      "theme": "coy",
      "css": true
    }]
  ]
}
